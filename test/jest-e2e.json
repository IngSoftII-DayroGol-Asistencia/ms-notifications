{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "apicreatelibs/send-grid/(.*)": "<rootDir>/../libs/send-grid/src/$1",
    "apicreatelibs/send-grid": "<rootDir>/../libs/send-grid/src",
    "^src/(.*)": "<rootDir>/../src/$1"
  }
}